<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .playersViewButtonHPanel {
      margin: 5px 0px 0x 5px;
    }
    .playersViewPlayersFlexTable {
      margin: 5px 0px 5px 0px;
    }

  </ui:style>
  <g:DialogBox ui:field="popupBox">
   <g:VerticalPanel >
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:HTML ui:field="picture">picture</g:HTML>
			<g:HTML ui:field="details">info</g:HTML>	
		</g:HorizontalPanel>
			<g:HTML ui:field="ratingTable">rating</g:HTML>
			<g:HTML ui:field="matchRatingTable">matches</g:HTML>
    </g:VerticalPanel>
    <g:HorizontalPanel>
    	<g:HTML ui:field="footer">View Player Sheet</g:HTML>
    	<g:Button ui:field="closeButton">Close</g:Button>
    </g:HorizontalPanel>
   </g:VerticalPanel>
   </g:DialogBox>

</ui:UiBinder>