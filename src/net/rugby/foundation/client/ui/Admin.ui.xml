<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>

	<g:HTMLPanel>
		<g:HorizontalPanel><g:Label>Status: </g:Label><g:HTML ui:field="textArea"></g:HTML></g:HorizontalPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel><g:Label>Feature ID:</g:Label><g:TextBox ui:field="featureID"/></g:HorizontalPanel>
			<g:HorizontalPanel><g:Label>Preamble:</g:Label><g:TextArea ui:field="featurePreamble"/></g:HorizontalPanel>
			<g:Button ui:field="setFeatureButton">Set Featured Group</g:Button>
		</g:VerticalPanel>
		<g:HorizontalPanel><g:TextArea ui:field="noLoginText"/><g:Button ui:field="noLoginSubmitButton">Save No Login Home Content</g:Button></g:HorizontalPanel>
		<g:HorizontalPanel><g:TextArea ui:field="noDraftText"/><g:Button ui:field="noDraftSubmitButton">Save No Draft Home Content</g:Button></g:HorizontalPanel>
		<g:HorizontalPanel><g:TextArea ui:field="noRoundText"/><g:Button ui:field="noRoundSubmitButton">Save No Round Home Content</g:Button></g:HorizontalPanel>
		<g:HorizontalPanel><g:TextArea ui:field="completeText"/><g:Button ui:field="completeSubmitButton">Save Complete Home Content</g:Button></g:HorizontalPanel>
		
		<g:HorizontalPanel><g:TextBox ui:field="adminEmail"></g:TextBox><g:Button ui:field="makeAdminButton">Make Admin</g:Button></g:HorizontalPanel>
		<g:HorizontalPanel><g:TextBox ui:field="superAdminEmail"></g:TextBox><g:Button ui:field="makeSuperAdminButton">Make Super Admin</g:Button></g:HorizontalPanel>
		<g:HorizontalPanel><g:Button styleName="{style.important}" ui:field="endRound" >End Round</g:Button>
		<g:Button styleName="{style.important}" ui:field="updateScores" >updateScores</g:Button>
		<g:Button styleName="{style.important}" ui:field="convertCompetition" >convertComps</g:Button>
		<g:Button styleName="{style.important}" ui:field="sendEmails" >email</g:Button></g:HorizontalPanel>
		<g:Button ui:field="close">Close</g:Button>
	</g:HTMLPanel>

</ui:UiBinder> 