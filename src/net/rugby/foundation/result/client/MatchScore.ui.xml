	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:Style>
		.no-margin-bot {
	        margin-bottom: 0px;
		}	
	</ui:Style>


	<b:Container>
		<b:Panel ui:field="scorePanel" addStyleNames='{style.no-margin-bot}'>
			<b:PanelHeader ui:field="header">
			</b:PanelHeader>
			<b:PanelBody ui:field="body">
			</b:PanelBody>
		</b:Panel>
	</b:Container>


</ui:UiBinder>