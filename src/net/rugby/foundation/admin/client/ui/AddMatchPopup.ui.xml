<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">



	<b:Well>

		<b:Form>
			<b:Label>Home Team Id</b:Label>
			<b:TextBox ui:field='homeTeamId'></b:TextBox>
			<b:Label>Visiting Team Id</b:Label>
			<b:TextBox ui:field='visitingTeamId'></b:TextBox>
			<b:Label>ESPN match Id</b:Label>
			<b:TextBox ui:field='espnMatchId'></b:TextBox>
			<b:Label>Espn League Id (leave blank for containing comp's espn league id)</b:Label>
			<b:TextBox ui:field='espnLeagueId'></b:TextBox>
			<b:Nav>
				<b:ButtonGroup ui:field='buttonBar'>
					<b:Button ui:field="save">Save</b:Button>
					<b:Button ui:field="cancel">Cancel</b:Button>
				</b:ButtonGroup>
			</b:Nav>
			
		</b:Form>

	</b:Well>
</ui:UiBinder> 