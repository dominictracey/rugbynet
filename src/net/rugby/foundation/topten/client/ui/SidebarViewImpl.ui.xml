	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">


	<g:HTMLPanel width="100%">
		
		<b:ListGroup ui:field="dashboardMenu">
			<b:ListGroupItem ui:field="sidebarProfile">
				
				<b:AnchorButton dataToggle="DROPDOWN" icon="COG" ui:field="profileButton">
