<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">

	<ui:style>
	</ui:style>

	<b:Panel ui:field="topPanel">
		<b:Row>
			<b:Column ui:field="captionPanel" size="8">
				    <b.html:Span ui:field="title"></b.html:Span>
					<b:Image ui:field="close" url="/resources/closeButton25.png"></b:Image>
			</b:Column>
		</b:Row>
		<b:Row>
			<b:Column size="8">
				<g:Image ui:field="image" />
				<g:HTML ui:field="playerInfo"></g:HTML>
			</b:Column>
		</b:Row>
		<b:Row>
			<b:Column size="8">
				<c:CellTable ui:field="ratingInfo" width="100%" />
				<g:HTML ui:field="content"></g:HTML>
			</b:Column>
		</b:Row>
	</b:Panel>
</ui:UiBinder>