<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" >

	<b:Panel ui:field="panel">
	    <b:PanelHeader>
	        <g:HTML ui:field="header"></g:HTML>
	    </b:PanelHeader>
	    <b:PanelBody ui:field="contentBody">
	        <g:HTML ui:field="content"></g:HTML>
	    </b:PanelBody>
	    <b:PanelFooter ui:field="panelFooter">
	        <b:ButtonGroup ui:field="buttonBar" >
	            <b:Button ui:field="editButton" type="PRIMARY">Edit</b:Button>
            </b:ButtonGroup>
        </b:PanelFooter>
	</b:Panel>

</ui:UiBinder>