<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.html'>

	<g:HTMLPanel width="100%" ui:field="topTenPanel">
			<b:ButtonGroup>
				<b:Button>Ratings</b:Button>
				<b:Button dataToggle="DROPDOWN" />
				<b:DropDownMenu ui:field="criteriaDropDown">

				</b:DropDownMenu>
			</b:ButtonGroup>

			<b:ButtonGroup>
				<b:Button>Week</b:Button>
				<b:Button dataToggle="DROPDOWN" />
				<b:DropDownMenu ui:field="weekDropDown">

				</b:DropDownMenu>
			</b:ButtonGroup>
			<g:HTML ui:field="listTitle"></g:HTML>

			<b:TabPanel tabPosition="LEFT" ui:field="tabPanel">
				<b:NavTabs ui:field="navTabs">
				</b:NavTabs>
				<b:TabContent ui:field="tabContent">			    
				</b:TabContent>
				<b.html:ClearFix />
			</b:TabPanel>
			
			<b:Panel ui:field="adminButtons">
			    <b:Button ui:field="createFeature" type="DEFAULT">Create Feature</b:Button>
			    <b:Button ui:field="promote" type="DEFAULT">Promote</b:Button>
			</b:Panel>
	</g:HTMLPanel>


</ui:UiBinder>