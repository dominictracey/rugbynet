<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.childDiv2 { /*float:left;*/ }
		.childDiv3 {
		position:relative;
		float:right;
		padding-left:15px;
		}
		.vertical-align {
	        display: flex;
    		align-items: center;
		}
	</ui:style>
	<b:Container ui:field="topTenPanel">
		<b:Row>
			<b:Column ui:field="topColumn" size="9">
				<b:Row>
					<b.html:Div ui:field="featuredTTL" addStyleNames='{style.childDiv3}'>
					</b.html:Div>
					<b.html:Div ui:field="featureBody">
					</b.html:Div>
				</b:Row>
<!-- 				<b:Row> -->
<!-- 					<b:Column size="3" offset="5"> -->
<!-- 						<b:Nav> -->
<!-- 							<b:ButtonGroup> -->
<!-- 								<b:Button size="LARGE" icon="BACKWARD" ui:field="prevButton">Prev</b:Button> -->
<!-- 								<b:Button size="LARGE" icon="FORWARD" ui:field="nextButton">Next</b:Button> -->
<!-- 							</b:ButtonGroup> -->
<!-- 						</b:Nav> -->
<!-- 					</b:Column> -->
<!-- 				</b:Row> -->
				<b:Row ui:field="row" addStyleNames='{style.vertical-align}'>
					<b:Column ui:field="left" size="1">
						<b:Anchor ui:field="left_arrow"></b:Anchor>
					</b:Column>
					<b:Column ui:field="prev" size="5">
						<b:Anchor ui:field="prevList"></b:Anchor>
					</b:Column>
					<b:Column ui:field="next" size="5">
						<b:Anchor ui:field="nextList"></b:Anchor>
					</b:Column>
					<b:Column ui:field="right" size="1">
						<b:Anchor ui:field="right_arrow"></b:Anchor>
					</b:Column>
				</b:Row>
				<b:Row>
					<b.html:Div ui:field="adminButtons">
						<b:Button ui:field="edit" type="PRIMARY">Edit</b:Button>
						<b:Button ui:field="publish" type="DANGER">Publish</b:Button>
						<b:Button ui:field="promote" type="PRIMARY">Promote</b:Button>
					</b.html:Div>
				</b:Row>
			</b:Column>
		</b:Row>
	</b:Container>


</ui:UiBinder>