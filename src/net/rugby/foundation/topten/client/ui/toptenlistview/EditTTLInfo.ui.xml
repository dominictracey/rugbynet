<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui">



	<b:Well>
		<b:Form>
			<b:Label>Title</b:Label>
			<b:TextBox ui:field='title' width="75%"></b:TextBox>
			<b:Label>Description</b:Label>
			<b:TextArea ui:field='description' width="100%"></b:TextArea>
			<b:Label>Twitter Description</b:Label>
			<b:TextBox ui:field='twitter' width="75%"></b:TextBox>

			<b:ButtonGroup>
				<b:Button ui:field="save" type="PRIMARY">Save</b:Button>
				<b:Button ui:field="cancel">Cancel</b:Button>
			</b:ButtonGroup>
		</b:Form>

	</b:Well>
</ui:UiBinder> 