<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
    		 xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    		 xmlns:c="urn:import:com.google.gwt.user.cellview.client" >
	<ui:style>
		.buttons {
			margin: 20px 20px;
		}
	</ui:style>
    <g:HTMLPanel width="100%" ui:field="topTenPanel">




    	<b:Row>
    	    <b:Column size ="12">
    	        <b:Row>
    	            <b:Column size="9" offset="2">
    	             	<c:CellTable ui:field="items" width="100%"/>
    	             </b:Column>
    	        </b:Row>
    	        <b:Row>
    	            
    	        </b:Row>
    	        <b:Row>
    	            <b:Column size="4" offset="4">
	    	            <b:Nav alignment="RIGHT">
	    	                <b:NavWidget  styleName="{style.buttons}">
	    	                    <b:Button size="LARGE" icon="BACKWARD" ui:field="prevButton">Prev</b:Button>
	    	                    <b:Button size="LARGE" icon="FORWARD" ui:field="nextButton">Next</b:Button>
	    	                    
	    	                </b:NavWidget>
	    	            </b:Nav>

    	            </b:Column>
    	        </b:Row>
    	    </b:Column>
		</b:Row>
		</g:HTMLPanel>


</ui:UiBinder>